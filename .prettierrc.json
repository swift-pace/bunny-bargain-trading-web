{
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "printWidth": 80,
    "tabWidth": 4,
    "arrowParens": "always",
    "semi": false,
    "bracketSameLine": false,
    "bracketSpacing": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "singleAttributePerLine": true
}
